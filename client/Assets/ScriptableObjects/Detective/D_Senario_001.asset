      normalizedPoint: {x: 0.5352564, y: 0.649}
      radius: 0.1
      uiHint: "\uCD08\uBC18\uC758 \uBD84\uC704\uAE30\uB294 \uB098\uC058\uC9C0 \uC54A\uC558\uC74C!"
      successDialogue:
      - speaker: "\uD0D0\uC815 \uACE0\uB09C"
        text: "\uC624 \uADF8\uB798 \uBC14\uB85C \uADF8\uAC70\uC57C!"
      - speaker: "\uD0D0\uC815 \uACE0\uB09C"
        text: "\uCD08\uBC18\uC758 \uBD84\uC704\uAE30\uB294 \uB098\uC058\uC9C0 \uC54A\uC558\uB2E4\uB294
          \uC810!\n\uC544\uC8FC \uC911\uC694\uD55C \uC0AC\uC2E4\uC774\uC57C."
      recapLines:
      - speaker: "\uD0D0\uC815 \uACE0\uB09C"
        text: "\uC6B0\uC120 \uC601\uC11C\uC591\uB3C4 \uBBFC\uD638\uC528\uC758 \uCCAB\uC778\uC0C1\uC774
          \uAF64 \uB9C8\uC74C\uC5D0 \uB4E4\uC5C8\uC5B4."
      - speaker: "\uD0D0\uC815 \uACE0\uB09C"
        text: "\uADF8\uB798\uC11C \uACC4\uC18D \uC6C3\uC73C\uBA70 \uB9AC\uC561\uC158\uC744
          \uD574 \uC900 \uAC70\uC9C0."
  - spriteName: D_D1_03
    lines:
    - speaker: "\uD0D0\uC815 \uACE0\uB09C"
      text: "\uC598\uAE30\uB97C \uB4E3\uB358 \uC911 \uC601\uC11C\uC528\uAC00 \uAC11\uC790\uAE30
