    evidence:
      normalizedPoint: {x: 0.2628, y: 0.2355}
      radius: 0.1
      uiHint: "\uC5BC\uAD74\uC744 \uCC21\uADF8\uB9B0 \uAC74 \uB808\uBAA8\uB124\uC774\uB4DC
